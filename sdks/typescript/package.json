{
  "name": "@llm-dev-ops/sentinel-sdk",
  "version": "0.1.0",
  "description": "Production-ready TypeScript/JavaScript SDK for LLM-Sentinel anomaly detection system",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "sentinel",
    "anomaly-detection",
    "observability",
    "monitoring",
    "telemetry",
    "kafka",
    "gpt",
    "claude",
    "openai",
    "anthropic",
    "llm-ops",
    "mlops"
  ],
  "author": "LLM-DevOps Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/llm-dev-ops/sentinel.git",
    "directory": "sdks/typescript"
  },
  "bugs": {
    "url": "https://github.com/llm-dev-ops/sentinel/issues"
  },
  "homepage": "https://github.com/llm-dev-ops/sentinel#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "kafkajs": "^2.2.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^4.5.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
